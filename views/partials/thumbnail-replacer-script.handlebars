<script>
    $(document).ready(() => {
        $blah = $("#blah");
        $bookList = $("#book-list");
        $.getJSON("{{{apiUrl}}}", function (data) {
            console.log(data);
            $(".book-thumbnail").each(function () {
                let $this = $(this);
                let isbn = $this.attr("data-isbn");
                if (data[isbn]) {
                    $this.attr("src", data[isbn].thumbnail_url);
                }
            });
        });
    });
</script>